    "@material-ui/styles": "^4.1.1",
    "apollo-boost": "^0.4.3",
    "clsx": "^1.0.4",
    "electron": "^5.0.6",
    "foreman": "^3.0.1",
    "gql": "^1.1.2",
    "graphql": "^14.4.0",
    "graphql-tag": "^2.10.1",
