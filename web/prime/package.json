{
  "name": "prime",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.2",
    "@fortawesome/pro-light-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@google-cloud/storage": "^3.0.2",
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "@material-ui/styles": "^4.1.1",
    "apollo-boost": "^0.4.3",
    "clsx": "^1.0.4",
    "dotenv": "^8.0.0",
    "gql": "^1.1.2",
    "graphql": "^14.4.0",
    "graphql-tag": "^2.10.1",
    "lodash.filter": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.pickby": "^4.6.0",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-app-rewire-yarn-workspaces": "^1.0.3",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.8.6",
    "react-particles-js": "^2.6.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-snap": "^1.23.0",
    "yarn": "^1.16.0"
  },
  "scripts": {
    "prep": "node ./set-environment && node node_modules/dotenv/config",
    "start": "npm run prep && react-scripts start",
    "build": "npm run prep && react-scripts build && npm run lib",
    "deploy": "node ./deploy.js",
    "test": "react-scripts  test",
    "eject": "react-scripts  eject"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      [
        "transform-assets",
        {
          "extensions": [
            "css",
            "svg"
          ],
          "name": "static/media/[name].[hash:8].[ext]"
        }
      ],
      "dynamic-import-node"
    ],
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "inlineCss": true
  }
}
